{
  "name": "@mazrean/tamamo-x-mcp",
  "version": "0.1.0",
  "description": "TypeScript MCP server integrating existing MCP servers as sub-agents",
  "type": "module",
  "main": "./dist/index.js",
  "bin": {
    "tamamo-x-mcp": "./dist/index.js"
  },
  "scripts": {
    "dev": "tsx src/index.ts",
    "build": "tsc && chmod +x dist/index.js",
    "test": "vitest",
    "lint": "eslint src",
    "format": "prettier --write src"
  },
  "keywords": [
    "mcp",
    "mcp-server",
    "agent",
    "sub-agent",
    "typescript"
  ],
  "author": "mazrean",
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "@mastra/core": "^0.1.0",
    "@modelcontextprotocol/sdk": "^0.5.0"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "tsx": "^4.7.0",
    "typescript": "^5.3.0",
    "vitest": "^1.0.0"
  }
}
